programa {
  funcao inicio() {
  cadeia m,v,n, turno
  escreva ("m ou v ou n:")
  leia (turno)
  escolha (turno){
  caso "m" :
  escreva("bom dia")
  pare
  caso "v" :
  escreva ("boa tarde")
  pare
  caso "n" :
  escreva ("boa noite")
  pare 
  caso  "":
  escreva ("turno invalido")
  pare
  }
  }
}
